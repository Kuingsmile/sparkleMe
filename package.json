{
  "name": "sparkleme",
  "version": "1.7.0",
  "description": "SparkleMe",
  "homepage": "https://github.com/Kuingsmile/sparkleMe",
  "author": "Kuingsmile",
  "type": "module",
  "main": "./out/main/index.js",
  "scripts": {
    "artifact": "node scripts/artifact.mjs",
    "build:linux": "electron-vite build && electron-builder --publish never --linux",
    "build:mac": "electron-vite build && electron-builder --publish never --mac",
    "build:win": "electron-vite build && electron-builder --publish never --win",
    "bump": "bump-version",
    "checksum": "node scripts/checksum.mjs",
    "cz": "git-cz",
    "dev": "electron-vite dev",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:style": "stylelint \"src/**/*.(vue|less|scss|css)\" --fix",
    "prepare": "node scripts/prepare.js",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "updater": "node scripts/updater.js"
  },
  "commitlint": {
    "extends": [
      "./node_modules/node-bump-version/dist/commitlint-node/index.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./node_modules/node-bump-version/.cz-config.cjs"
    }
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@heroui/react": "2.8.7",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/crypto-js": "^4.2.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "express": "^5.2.1",
    "file-icon": "^6.0.0",
    "file-icon-info": "^1.1.1",
    "iconv-lite": "^0.7.1",
    "is-cidr": "^6.0.1",
    "is-ip": "^5.0.1",
    "webdav": "^5.8.0",
    "ws": "^8.18.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/adm-zip": "^0.5.7",
    "@types/express": "^5.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "@types/plist": "^3.0.5",
    "@types/pubsub-js": "^1.8.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "cron-validator": "^1.4.0",
    "driver.js": "^1.4.0",
    "electron": "^37.10.3",
    "electron-builder": "26.0.12",
    "electron-vite": "^5.0.0",
    "electron-window-state": "^5.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "form-data": "^4.0.5",
    "framer-motion": "12.23.26",
    "jsonc-eslint-parser": "^2.4.2",
    "lodash": "^4.17.21",
    "meta-json-schema": "^1.19.18",
    "monaco-yaml": "^5.4.0",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "node-bump-version": "^2.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "pubsub-js": "^1.9.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-monaco-editor": "^0.59.0",
    "react-router-dom": "^7.11.0",
    "react-virtuoso": "^4.18.1",
    "recharts": "^3.6.0",
    "stylelint": "^16.26.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^7.0.1",
    "swr": "^2.3.8",
    "tailwindcss": "^4.1.18",
    "tar": "^7.5.2",
    "tsx": "^4.21.0",
    "types-pac": "^1.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vite-plugin-monaco-editor": "1.1.0"
  },
  "packageManager": "pnpm@10.15.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ],
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    },
    "patchedDependencies": {
      "vite-plugin-monaco-editor@1.1.0": "patches/vite-plugin-monaco-editor@1.1.0.patch"
    }
  }
}
